{
    "name": "moiutil",
    "version": "0.0.0-development",
    "description": "A simple utility library for MOI Protocol",
    "main": "dist/index.js",
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bin": {
        "moi": "dist/index.js"
    },
    "scripts": {
        "link:local": "pnpm link .",
        "build": "esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js",
        "watch": "(pnpm link . || true) && esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js --watch",
        "cm": "cz",
        "semantic-release": "semantic-release --branches main"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/harshrastogiexe/moi-cli.git"
    },
    "dependencies": {
        "typescript": "^5.3.3"
    },
    "devDependencies": {
        "@types/node": "^20.11.16",
        "commitizen": "^4.3.0",
        "cz-conventional-changelog": "^3.3.0",
        "esbuild": "^0.20.0",
        "semantic-release": "^23.0.2"
    }
}